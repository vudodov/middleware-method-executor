<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <Nullable>enable</Nullable>
        <PackageId>middleware-method-executor</PackageId>
        <Title>The Middleware Method Executor</Title>
        <Authors>Valerii Udodov &lt;vudodov@gmail.com&gt;</Authors>
        <PackageLicenseUrl>https://github.com/vudodov/middleware-method-executor/blob/master/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/vudodov/middleware-method-executor</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="3.1.2" />
      <PackageReference Include="System.IO.Pipelines" Version="4.7.0" />
      <PackageReference Include="System.Text.Json" Version="4.7.1" />
    </ItemGroup>

</Project>
